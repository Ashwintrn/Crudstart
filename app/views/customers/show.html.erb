<table>
  <tr>
    <th>id</th>
    <th>customer_id</th>
    <th>item_id</th>
    <th>quantity</th>
    <th>total_cost</th>
    <th>created_at</th>
    <th>updated_at</th>
    <th></th>
  </tr>
 
  <% @orders.each do |order| %>
    <tr>
      <td><%= order.id %></td>
      <td><%= order.customer_id %></td>
      <td><%= order.item_id %></td>
      <td><%= order.quantity %></td>
      <td><%= order.total_cost %></td>
      <td><%= order.created_at %></td>
      <td><%= order.updated_at %></td>
      <td></td>
    </tr>
  <% end %>
</table>
